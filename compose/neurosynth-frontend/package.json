{
    "name": "neurosynth",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@auth0/auth0-react": "^1.6.0",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@handsontable/react": "^11.0.1",
        "@mui/icons-material": "^5.0.1",
        "@mui/material": "^5.7.0",
        "@mui/styles": "^5.5.1",
        "@mui/system": "^5.5.1",
        "@mui/x-data-grid": "^5.10.0",
        "@reactour/tour": "^2.10.3",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.20.15",
        "@types/react": "^17.0.13",
        "@types/react-dom": "^17.0.8",
        "@types/react-router-dom": "^5.1.8",
        "axios": "^0.21.1",
        "handsontable": "^11.1.0",
        "notistack": "^2.0.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.38.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "typescript": "^4.3.5",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start:dev": "env-cmd -f .env.dev react-scripts start",
        "build:dev": "env-cmd -f .env.dev react-scripts build",
        "start:staging": "env-cmd -f .env.staging react-scripts start",
        "build:staging": "env-cmd -f .env.staging react-scripts build",
        "build:prod": "env-cmd -f .env.prod react-scripts build",
        "test": "react-scripts test",
        "test-with-coverage": "npm run test -- --coverage --watchAll=false",
        "cy:e2e-headless": "cypress run",
        "cy:e2e-browser": "cypress open",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "//jestComments": {
        "//resetMocks": "we need to set this flag because of this: https://github.com/facebook/create-react-app/issues/9935"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(css|less)$": "<rootDir>/src/testing/stylemock.js"
        },
        "resetMocks": false
    },
    "devDependencies": {
        "@testing-library/dom": "^8.3.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "cypress": "^8.6.0",
        "env-cmd": "^10.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.4.0",
        "react-error-overlay": "^6.0.9"
    }
}
