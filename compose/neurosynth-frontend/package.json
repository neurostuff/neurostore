        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@handsontable/react": "^12.3.3",
        "@hello-pangea/dnd": "^16.0.1",
        "@mui/icons-material": "^5.0.1",
        "@mui/material": "^5.7.0",
        "@mui/styles": "^5.5.1",
        "@mui/system": "^5.5.1",
        "@mui/x-data-grid": "^5.10.0",
        "@reactour/tour": "^2.10.3",
        "@sentry/react": "^7.48.0",
        "@tanstack/react-table": "^8.20.5",
        "@types/react": "^17.0.13",
        "@types/react-dom": "^17.0.8",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-window": "^1.8.5",
        "@types/uuid": "^9.0.0",
        "@vitejs/plugin-react": "^4.3.3",
        "axios": "^0.28.0",
        "fast-xml-parser": "^4.4.1",
        "handsontable": "^12.3.3",
        "html-to-image": "^1.11.11",
        "i": "^0.3.7",
        "jose": "^4.15.5",
        "notistack": "^2.0.5",
        "npm": "^10.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.39.2",
        "react-router-dom": "^6.22.3",
        "react-window": "^1.8.8",
        "reactflow": "^11.5.3",
        "typescript": "^4.3.5",
        "uuid": "^9.0.0",
        "vite": "^5.4.10",
        "vite-tsconfig-paths": "^5.0.1",
        "web-vitals": "^1.1.2",
        "zustand": "^4.3.3"
    },
    "scripts": {
        "start:dev": "vite --mode dev",
        "build:dev-local": "vite build --mode dev",
        "start-ci:dev": "serve -s -d -n -p 3000 build",
        "start-ci:dev-local": "serve -s -d -n -p 3000 build",
        "start:staging": "vite --mode staging",
        "build:staging": "vite build --mode staging",
        "build:prod": "vite build --mode production",
        "test": "vite test --watchAll=false",
        "test-with-coverage": "npm run test -- --coverage --watchAll=false",
        "cy:e2e-headless": "cypress run",
        "cy:e2e-headless-dev": "env-cmd -f .env.dev cypress run",
        "cy:e2e-headless-staging": "env-cmd -f .env.staging cypress run",
        "cy:e2e-browser-staging": "env-cmd -f .env.staging cypress open -b chrome --e2e",
        "cy:e2e-browser-dev": "env-cmd -f .env.dev cypress open -b chrome --e2e",
        "cy:install": "cypress install",
        "cy:ci": "start-server-and-test start-ci:dev 3000 cy:e2e-headless"
    },
    "type": "module",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "//jestComments": {
        "//resetMocks": "we need to set this flag because of this: https://github.com/facebook/create-react-app/issues/9935"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(css|less)$": "<rootDir>/src/testing/stylemock.js"
        },
        "resetMocks": false
    },
    "devDependencies": {
        "@testing-library/dom": "^8.3.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^29.5.14",
        "@types/node": "^18.19.63",
        "cypress": "^13.2.0",
        "env-cmd": "^10.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^2.4.0",
        "react-error-overlay": "^6.0.9",
        "serve": "^14.0.1",
        "start-server-and-test": "^1.14.0",
        "ts-jest": "^29.2.5"
    }
}
